<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://maven.apache.org/changes/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
  <properties>
    <title>Changelog</title>
    <author email="schulte2005@users.sourceforge.net">Christian Schulte
    </author>
  </properties>
  <body>
    <release version="1.0" date="2007-06-01"
             description="First stable release."/>
    <release version="1.0.1" date="2007-08-01"
             description="Maintenance release.">
      <action dev="schulte2005" type="fix"><![CDATA[
      Updated the scope of the <code>servlet-api</code> dependency from
      <code>compile</code> to <code>provided</code>.]]>
      </action>
    </release>
    <release version="1.0.2" date="2008-03-10"
             description="Maintenance release.">
      <action dev="schulte2005" type="fix"><![CDATA[
      Added various missing serial version UID fields and updated to latest
      mojo releases.]]>
      </action>
    </release>
    <release version="1.0.3" date="2008-07-04"
             description="Maintenance release.">
      <action dev="schulte2005" type="update"><![CDATA[
            Maven POM updates.]]>
      </action>
    </release>
    <release version="1.0.4" date="2008-07-13"
             description="Maintenance release.">
      <action dev="schulte2005" type="update"><![CDATA[
            Maven POM updates.]]>
      </action>
    </release>
    <release version="1.1" date="2008-10-27"
             description="Maintenance release.">
      <action dev="schulte2005" type="update"><![CDATA[
      Updated to latest source generators and implemented methods introduced in
      jDTAUS Core API 1.5.]]>
      </action>
    </release>
    <release version="1.8" date="2008-12-28"
             description="Maintenance release.">
      <action dev="schulte2005" type="update"><![CDATA[
      Added an implementation overwriting the system's default
      <code>java.util.Locale</code> implementation to use the locale of a
      request in service.]]>
      </action>
    </release>
    <release version="1.9" date="2009-07-02"
             description="Maintenance release.">
      <action dev="schulte2005" type="update"><![CDATA[
      Updated any references to the jDTAUS server to point to the SF.net
      servers.]]></action>
      <action dev="schulte2005" type="fix"><![CDATA[
      Corrected GPG signatures.]]></action>
    </release>
    <release version="1.9.1" date="2009-11-09"
             description="Maintenance release.">
      <action dev="schulte2005" type="update"><![CDATA[
      Maven POM updates.]]></action>
    </release>
  </body>
