  <container:messages>
    <container:message container:name="logResize">
      <container:documentation container:defaultLanguage="en">
        <container:text container:language="en"><![CDATA[Information about the size of the internal buffer.]]></container:text>
      </container:documentation>
      <container:template container:defaultLanguage="en">
        <container:text container:language="en"><![CDATA[current buffer size: {0, number} byte]]></container:text>
        <container:text container:language="de"><![CDATA[aktuelle Puffergröße: {0, number} Byte]]></container:text>
      </container:template>
      <container:arguments>
        <container:argument container:index="0" container:name="bufferSize" container:type="number">
          <container:documentation container:defaultLanguage="en">
            <container:text container:language="en"><![CDATA[The current size of the internal buffer.]]></container:text>
          </container:documentation>
        </container:argument>
      </container:arguments>
    </container:message>
    <container:message container:name="readBypassesCache">
      <container:documentation container:defaultLanguage="en">
        <container:text container:language="en"><![CDATA[Information about a misconfigured cache.]]></container:text>
      </container:documentation>
      <container:template container:defaultLanguage="en">
        <container:text container:language="en"><![CDATA[A read operation bypassed the cache. Consider increasing the cache. Current block size is {0,number} and current number of cache blocks is {1,number}. {2,number} bytes could not be cached.]]></container:text>
        <container:text container:language="de"><![CDATA[Eine Lese-Operation umging den Cache. Cache zu klein dimensioniert. Aktuelle Blockgröße ist {0,number} und Cache umfaßt {1,number} Blöcke. {2,number} Bytes konnten nicht zwischengespeichert werden.]]></container:text>
      </container:template>
      <container:arguments>
        <container:argument container:index="0" container:name="blockSize" container:type="number">
          <container:documentation container:defaultLanguage="en">
            <container:text container:language="en"><![CDATA[The current block size in use.]]></container:text>
          </container:documentation>
        </container:argument>
        <container:argument container:index="1" container:name="cacheBlocks" container:type="number">
          <container:documentation container:defaultLanguage="en">
            <container:text container:language="en"><![CDATA[The current number of blocks in use.]]></container:text>
          </container:documentation>
        </container:argument>
        <container:argument container:index="2" container:name="uncachedBytes" container:type="number">
          <container:documentation container:defaultLanguage="en">
            <container:text container:language="en"><![CDATA[The number of bytes bypassing caching.]]></container:text>
          </container:documentation>
        </container:argument>
      </container:arguments>
    </container:message>
    <container:message container:name="writeBypassesCache">
      <container:documentation container:defaultLanguage="en">
        <container:text container:language="en"><![CDATA[Information about a misconfigured cache.]]></container:text>
      </container:documentation>
      <container:template container:defaultLanguage="en">
        <container:text container:language="en"><![CDATA[A write operation bypassed the cache. Consider increasing the cache. Current block size is {0,number} and current number of cache blocks is {1,number}. {2,number} bytes could not be cached.]]></container:text>
        <container:text container:language="de"><![CDATA[Eine Schreib-Operation umging den Cache. Cache zu klein dimensioniert. Aktuelle Blockgröße ist {0,number} und Cache umfaßt {1,number} Blöcke. {2,number} Bytes konnten nicht zwischengespeichert werden.]]></container:text>
      </container:template>
      <container:arguments>
        <container:argument container:index="0" container:name="blockSize" container:type="number">
          <container:documentation container:defaultLanguage="en">
            <container:text container:language="en"><![CDATA[The current block size in use.]]></container:text>
          </container:documentation>
        </container:argument>
        <container:argument container:index="1" container:name="cacheBlocks" container:type="number">
          <container:documentation container:defaultLanguage="en">
            <container:text container:language="en"><![CDATA[The current number of blocks in use.]]></container:text>
          </container:documentation>
        </container:argument>
        <container:argument container:index="2" container:name="uncachedBytes" container:type="number">
          <container:documentation container:defaultLanguage="en">
            <container:text container:language="en"><![CDATA[The number of bytes bypassing caching.]]></container:text>
          </container:documentation>
        </container:argument>
      </container:arguments>
    </container:message>
    <container:message container:name="alreadyClosed">
      <container:documentation container:defaultLanguage="en">
        <container:text container:language="en"><![CDATA[Message stating that an instance is already closed.]]></container:text>
      </container:documentation>
      <container:template container:defaultLanguage="en">
        <container:text container:language="en"><![CDATA[Instance closed - cannot perform I/O.]]></container:text>
        <container:text container:language="de"><![CDATA[Instanz geschlossen - keine E/A-Operationen möglich.]]></container:text>
      </container:template>
    </container:message>
    <container:message container:name="unknownTaskEventType">
      <container:documentation container:defaultLanguage="en">
        <container:text container:language="en"><![CDATA[Message stating that an unknown task event got ignored.]]></container:text>
      </container:documentation>
